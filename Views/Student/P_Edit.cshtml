@model DemoUI.Models.EM_Student;

Edit Modal HTML
<div class="modal-dialog">
    <div class="modal-content">
        <form asp-action="P_Edit" id="form_data" asp-antiforgery="true">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="modal-header">
                <h4 class="modal-title">Edit Student</h4>
            </div>
            <div class="modal-body">
                <div class="form-group">
                    <label asp-for="id" class="col-md-2 control-label"></label>
                    <input asp-for="id" class="form-control">
                </div> 
                <div class="form-group">
                    <label asp-for="lastName" class="col-md-2 control-label"></label>
                    <input asp-for="lastName" class="form-control">
                </div>
                <div class="form-group">
                    <label asp-for="gender" class="col-md-2 control-label"></label>
                    <input asp-for="gender" class="form-control">
                </div>
                <div class="form-group">
                    <label asp-for="email" class="col-md-2 control-label"></label>
                    <input asp-for="email" class="form-control">
                </div>
                <div class="form-group">
                    <label asp-for="avatar" class="col-md-2 control-label"></label>
                    <input asp-for="avatar" class="form-control">
                </div>
                <div class="form-group">
                    <label asp-for="className" class="col-md-2 control-label"></label>
                    <input asp-for="className" class="form-control">
                </div>
                <div class="form-group">
                    <label asp-for="schoolName" class="col-md-2 control-label"></label>
                    <input asp-for="schoolName" class="form-control">
                </div>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
                <input type="button"value="Cancel" class="btn btn-outline-secondary waves-effect " onclick="BackToTable('#div_main_table', '#div_view_panel');"/>
            </div>
        </form>
    </div>
